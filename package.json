{
  "name": "next-react-web-components",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext js,ts,tsx src/",
    "prettier-check": "prettier \"**/*.ts{,x}\" --check",
    "pre-commit": "npm run lint && npm run prettier-check",
    "pre-push": "npm run build && npm run deploy",
    "dev": "next dev",
    "build": "rm -rf out && next build && next export",
    "deploy": "gh-pages -d out"
  },
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "^2.25.3",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@connectedhomes/nucleus": "^2.2.12",
    "next": "^12.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
